{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "test": "exit 0",
    "integration": "exit 0",
    "start": "npm run db:up && node dist/app.js",
    "dev:db": "docker run --name db -e POSTGRES_PASSWORD=${DB_PASS} -e POSTGRES_USER=${DB_USER} -p ${DB_PORT}:${DB_PORT} -d postgres:alpine",
    "dev": "concurrently --kill-others 'prisma generate --watch' 'ts-node-dev --no-notify src/app.ts'",
    "generate": "prisma generate",
    "db": "npm run db:migrate && npm run db:up",
    "db:migrate": "prisma migrate save --create-db --experimental",
    "db:up": "prisma migrate up --create-db --experimental",
    "db:format": "prisma format",
    "db:studio": "prisma studio --experimental --port 3000",
    "lint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:ci": "eslint './src/**/*.{ts,tsx}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/connect-redis": "0.0.14",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/http-errors": "^1.6.3",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.5",
    "@types/redis": "^2.8.21",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.33.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "dependencies": {
    "@prisma/cli": "^2.0.0-beta.6",
    "@prisma/client": "^2.0.0-beta.6",
    "argon2": "^0.26.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compose-middleware": "^5.0.1",
    "connect-redis": "^4.0.4",
    "env-var": "^6.0.4",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.17.1",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.10.0",
    "redis": "^3.0.2",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^3.9.3",
    "winston": "^3.2.1"
  }
}
